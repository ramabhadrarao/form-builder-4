{
  "name": "enterprise-nocode-system",
  "version": "1.0.0",
  "description": "Enterprise-grade No-Code Application Management System",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "seed": "node server/seeders/index.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "multer-gridfs-storage": "^5.0.2",
    "sharp": "^0.32.6",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "gridfs-stream": "^1.1.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "socket.io": "^4.7.4",
    "redis": "^4.6.11",
    "agenda": "^5.0.0",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0"
  },
  "keywords": [
    "no-code",
    "form-builder",
    "workflow-engine",
    "enterprise",
    "management-system"
  ],
  "author": "Enterprise Development Team",
  "license": "MIT"
}